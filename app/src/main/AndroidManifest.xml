<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/sprint_you_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/sprint_you_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.SprintYou">
        <activity
            android:name="com.termproject.sprintyou.ui.auth.SignupActivity"
            android:exported="false" />
        <activity
            android:name="com.termproject.sprintyou.ui.auth.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.termproject.sprintyou.ui.history.CalendarActivity"
            android:exported="false" />
        <activity
            android:name="com.termproject.sprintyou.ui.timer.TimerActivity"
            android:exported="false" />
        <activity
            android:name="com.termproject.sprintyou.ui.setup.SetupActivity"
            android:exported="false" />
        <activity
            android:name="com.termproject.sprintyou.ui.goal.GoalSettingActivity"
            android:exported="false" />
        <activity
            android:name="com.termproject.sprintyou.ui.home.HomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>